{
  "name": "dilab/resumable.php",
  "description": "PHP package for Resumable.js",
  "authors": [
    {
      "name": "xu ding",
      "email": "thedilab@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "cakephp/filesystem": "^3.0",
    "monolog/monolog": "^2.0",
    "ondrej-vrto/php-filename-sanitize": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~10.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Dilab\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dilab\\Test\\": "test/src/"
    }
  },
  "config": {
    "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
